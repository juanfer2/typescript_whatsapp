    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cpx": "^1.5.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.3",
    "graphql-import-node": "^0.0.4",
    "graphql-request": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "prisma": "^3.0.2",
    "redis": "^3.1.2",
