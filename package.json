{
  "name": "whastapp-api",
  "version": "1.0.0",
  "main": "src/index.js",
  "author": "juanfer",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "build": "rm -rf dist && npx tsc -p tsconfig.json && npx cpx './src/graphql/schemas/**/*.graphql' ./dist/graphql/schemas -u",
    "start": "node dist/index.js",
    "postinstall": "yarn build",
    "dev": "nodemon src/index.ts",
    "migration-pull-print": "npx prisma db pull --print",
    "prisma-generate": "npx prisma generate",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-watch": "jest --watch",
    "codegen": "graphql-codegen --config codegen.yml",
    "predev": "yarn codegen"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-resolvers": "2.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.5",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.27.7",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.172",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.9.1",
    "apollo-server-express": "^3.0.0-rc.1",
    "apollo-server-integration-testing": "^3.0.0",
    "babel-jest": "^27.2.5",
    "graphql-tools": "^8.2.0",
    "jest": "^27.2.5",
    "jest-mock-extended": "^2.0.4",
    "jest-transform-graphql": "^2.1.0",
    "lint": "^0.7.0",
    "logform": "^2.2.0",
    "nodemon": "^2.0.12",
    "sidekiq": "^1.1.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3",
    "winston": "^3.3.3"
  },
  "dependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
